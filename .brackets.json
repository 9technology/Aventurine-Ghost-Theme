{
    "path": {
        "assets/sass/*.scss": {
            "sass.enabled": true,
            "sass.options": {
                "outputDir": "../css/",
                "imagePath": null,
                "sourceComments": "map",
                "outputStyle": "nested"
            },
            "brackets-git.gitPath": "git",
            "useTabChar": false,
            "linting.collapsed": false
        }
    }
}
